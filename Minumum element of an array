#include<stdio.h>
int main()
{
    int a[100],n,i,m,j;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        m=a[i];
        for(j=i;j>=0;j--)
        {
            if(a[j]<m)
            {
                m=a[j];
            }
            else
            {
                m=a[i];
            }
        }
    }
    printf("%d",m);
}
